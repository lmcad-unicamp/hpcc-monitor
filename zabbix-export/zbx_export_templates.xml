                                    <type>JSONPATH</type>
                                    <params>$.[?(@.fsname=='{#FSNAME}')].pused.first()</params>
                                    <error_handler>CUSTOM_VALUE</error_handler>
                                    <error_handler_params>-1</error_handler_params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>vfs.fs.infos</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>vfs.fs.infos</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#DEV}</lld_macro>
                            <path>$.dev</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#FSNAME}</lld_macro>
                            <path>$.fsname</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#FSTYPE}</lld_macro>
