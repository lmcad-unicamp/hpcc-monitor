            <discovery_rules>
                <discovery_rule>
                    <name>Discovery Net Interfaces</name>
                    <type>DEPENDENT</type>
                    <key>net.if.discovery</key>
                    <delay>0</delay>
                    <lifetime>600d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Net Inbound Interface {#IFNAME}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>net.if.in[{#IFNAME},bytes]</key>
                            <history>600d</history>
                            <units>bs</units>
                        </item_prototype>
                        <item_prototype>
                            <name>Net Outbound Interface {#IFNAME}</name>
                            <type>ZABBIX_ACTIVE</type>
                            <key>net.if.out[{#IFNAME},bytes]</key>
                            <history>600d</history>
                            <units>bs</units>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>net.if.infos</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#IFNAME}</lld_macro>
                            <path>$.interface</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
                <discovery_rule>
                    <name>Discovery Filesystems</name>
                    <type>DEPENDENT</type>
                    <key>vfs.fs.discovery</key>
                    <delay>0</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#FSTYPE}</macro>
                                <value>^(btrfs|ext2|ext3|ext4|reiser|xfs|ffs|ufs|jfs|jfs2|vxfs|hfs|apfs|refs|ntfs|fat32|zfs)$</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>Free Space of dev {#DEV} - fs {#FSNAME} type {#FSTYPE}</name>
                            <type>DEPENDENT</type>
                            <key>vfs.fs.usage.free[{#DEV}]</key>
                            <delay>0</delay>
                            <history>600d</history>
                            <value_type>FLOAT</value_type>
                            <units>Bytes</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[?(@.fsname=='{#FSNAME}')].pfree.first()</params>
                                    <error_handler>CUSTOM_VALUE</error_handler>
                                    <error_handler_params>-1</error_handler_params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>vfs.fs.infos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Total Space of dev {#DEV} - fs {#FSNAME} type {#FSTYPE}</name>
                            <type>DEPENDENT</type>
                            <key>vfs.fs.usage.space[{#DEV}]</key>
                            <delay>0</delay>
                            <history>600d</history>
                            <units>bytes</units>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[?(@.fsname=='{#FSNAME}')].size.first()</params>
                                    <error_handler>CUSTOM_VALUE</error_handler>
                                    <error_handler_params>-1</error_handler_params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>vfs.fs.infos</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Used Space of dev {#DEV} - fs {#FSNAME} type {#FSTYPE}</name>
                            <type>DEPENDENT</type>
                            <key>vfs.fs.usage.used[{#DEV}]</key>
                            <delay>0</delay>
                            <history>600d</history>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <preprocessing>
